    "feathers": "^2.0.3",
    "feathers-configuration": "^0.3.3",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "nuxt": "^2.0.0",
    "nuxt-social-meta": "0.0.1",
    "sass-loader": "^7.1.0",
    "vue-content-loader": "^0.2.1",
    "vue-goodshare": "^1.1.0",
    "vue-google-auth": "^1.0.5",
    "vue-i18n": "^8.1.0",
    "vue-moment": "^4.0.0",
    "vue-smoothscroll": "^0.2.0",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
