{
  "name": "nuxtjs-starter",
  "version": "1.0.0",
  "description": "The starter for Nuxt.js project with feathers",
  "author": "ken",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
